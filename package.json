{
  "name": "reaction-swag-shop",
  "version": "0.0.1",
  "description": "A Reaction Commerce plugin that creates swagshop.reactioncommerce.com",
  "main": "register.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reactioncommerce/reaction-swag-shop.git"
  },
  "keywords": [
    "reaction",
    "commerce",
    "examples",
    "plugins"
  ],
  "author": "Reaction Commerce",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/reactioncommerce/reaction-swag-shop/issues"
  },
  "homepage": "https://github.com/reactioncommerce/reaction-swag-shop#readme",
  "dependencies": {
    "buffer-stream-reader": "^0.1.1"
  },
  "babel": {
    "plugins": [
      "transform-class-properties",
      "dynamic-import-node",
      "babel-plugin-inline-import",
      [
        "lodash",
        {
          "id": [
            "lodash",
            "recompose"
          ]
        }
      ],
      [
        "module-resolver",
        {
          "root": [
            "./"
          ],
          "alias": {
            "@reactioncommerce/reaction-collections": "./imports/plugins/core/collections",
            "@reactioncommerce/reaction-components": "./imports/plugins/core/components/lib",
            "@reactioncommerce/reaction-graphql-utils": "./imports/plugins/core/graphql/server/resolvers/util",
            "@reactioncommerce/reaction-graphql-xforms": "./imports/plugins/core/graphql/server/resolvers/xforms",
            "@reactioncommerce/reaction-router": "./imports/plugins/core/router/lib",
            "@reactioncommerce/reaction-ui": "./imports/plugins/core/ui/client/components",
            "underscore": "lodash"
          }
        }
      ]
    ],
    "env": {
      "devserver": {
        "presets": [
          [
            "@babel/env",
            {
              "targets": {
                "node": "current"
              }
            }
          ],
          [
            "@babel/preset-stage-2",
            {
              "decoratorsLegacy": true
            }
          ]
        ],
        "plugins": [
          "babel-plugin-inline-import",
          [
            "module-resolver",
            {
              "root": [
                "./"
              ],
              "alias": {
                "@reactioncommerce/reaction-collections": "./imports/plugins/core/collections",
                "@reactioncommerce/reaction-components": "./imports/plugins/core/components/lib",
                "@reactioncommerce/reaction-graphql-utils": "./imports/plugins/core/graphql/server/resolvers/util",
                "@reactioncommerce/reaction-graphql-xforms": "./imports/plugins/core/graphql/server/resolvers/xforms",
                "@reactioncommerce/reaction-router": "./imports/plugins/core/router/lib",
                "@reactioncommerce/reaction-ui": "./imports/plugins/core/ui/client/components",
                "meteor": "./.reaction/devserver/mockmeteor",
                "underscore": "lodash"
              }
            }
          ]
        ]
      },
      "jesttest": {
        "presets": [
          [
            "@babel/env",
            {
              "targets": {
                "node": "current"
              }
            }
          ],
          [
            "@babel/preset-stage-2",
            {
              "decoratorsLegacy": true
            }
          ]
        ],
        "plugins": [
          "rewire-exports",
          "babel-plugin-inline-import",
          [
            "module-resolver",
            {
              "root": [
                "./"
              ],
              "alias": {
                "@reactioncommerce/reaction-collections": "./imports/plugins/core/collections",
                "@reactioncommerce/reaction-components": "./imports/plugins/core/components/lib",
                "@reactioncommerce/reaction-graphql-utils": "./imports/plugins/core/graphql/server/resolvers/util",
                "@reactioncommerce/reaction-graphql-xforms": "./imports/plugins/core/graphql/server/resolvers/xforms",
                "@reactioncommerce/reaction-router": "./imports/plugins/core/router/lib",
                "@reactioncommerce/reaction-ui": "./imports/plugins/core/ui/client/components",
                "meteor": "./imports/test-utils/__mocks__/meteor",
                "underscore": "lodash"
              }
            }
          ]
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": "@reactioncommerce"
  },
  "devDependencies": {
    "@reactioncommerce/eslint-config": "^1.0.1",
    "eslint": "^4.17.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1"
  }
}
